{
	"compilerOptions": {
		"allowJs": true,
		"module": "commonjs",
		"noImplicitAny": false,
		"sourceMap": true,
		"baseUrl": ".",
		"outDir": "dist",
		"rootDir": "src",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"paths": {
			"*": ["node_modules/*"]
		}
	},
	"include": ["src/**/*"]
}
